{
  "name": "github-action",
  "version": "0.0.0",
  "private": true,
  "description": "yet another github action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "set NODE_OPTIONS=--openssl-legacy-provider && ncc build --source-map --license licenses.txt",
    "test": "jest",
    "test:w": "jest --watchAll",
    "all": "set NODE_OPTIONS=--openssl-legacy-provider && npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "github actions"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/rest": "^19.0.5"
  },
  "devDependencies": {
    "@octokit/types": "^8.0.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/parser": "^5.50.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^8.28.0",
    "eslint-plugin-github": "^4.4.1",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  }
}
